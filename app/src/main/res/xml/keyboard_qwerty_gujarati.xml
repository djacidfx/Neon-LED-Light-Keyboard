<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:horizontalGap="0%p"
    android:keyHeight="52dp"
    android:verticalGap="4dp">
    <Row
        android:horizontalGap="0.1%p"
        android:keyWidth="8.1%p">
        <Key
            android:horizontalGap="1%p"
            android:keyEdgeFlags="left"
            android:keyLabel="ૌ" />
        <Key android:keyLabel="ૈ" />
        <Key android:keyLabel="ા" />
        <Key android:keyLabel="ી" />
        <Key android:keyLabel="ૂ" />
        <Key android:keyLabel="બ" />
        <Key android:keyLabel="હ" />
        <Key android:keyLabel="ગ" />
        <Key android:keyLabel="દ" />
        <Key android:keyLabel="જ" />
        <Key android:keyLabel="ડ" />
        <Key
            android:keyEdgeFlags="right"
            android:keyLabel="઼" />
    </Row>
    <Row
        android:horizontalGap="0.1%p"
        android:keyWidth="8.8%p">
        <Key
            android:horizontalGap="1%p"
            android:keyEdgeFlags="left"
            android:keyLabel="ો" />
        <Key android:keyLabel="ે" />
        <Key android:keyLabel="્" />
        <Key android:keyLabel="િ" />
        <Key android:keyLabel="ુ" />
        <Key android:keyLabel="પ" />
        <Key android:keyLabel="ર" />
        <Key android:keyLabel="ક" />
        <Key android:keyLabel="ત" />
        <Key android:keyLabel="ચ" />
        <Key
            android:keyEdgeFlags="right"
            android:keyLabel="ટ" />
    </Row>
    <Row
        android:horizontalGap="0.1%p"
        android:keyWidth="9.7%p">
        <Key
            android:codes="-1"
            android:horizontalGap="1%p"
            android:isModifier="true"
            android:isSticky="true"
            android:keyWidth="13.5%p"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/ic_shiftup" />
        <Key
            android:horizontalGap="1%p"
            android:keyLabel="ં" />
        <Key android:keyLabel="મ" />
        <Key android:keyLabel="ન" />
        <Key android:keyLabel="વ" />
        <Key android:keyLabel="લ" />
        <Key android:keyLabel="સ" />
        <Key android:keyLabel="ય" />
        <Key
            android:codes="-5"
            android:horizontalGap="1%p"
            android:isRepeatable="true"
            android:keyWidth="14%p"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/ic_backspace" />
    </Row>
    <Row
        android:horizontalGap="0.1%p"
        android:keyWidth="9.7%p"
        android:rowEdgeFlags="bottom">
        <Key
            android:codes="-2"
            android:horizontalGap="1%p"
            android:keyWidth="14.7%p"
            android:keyLabel="123" />
        <Key
            android:codes="-101"
            android:keyIcon="@drawable/icon_emoj1" />
        <Key
            android:codes="32"
            android:isRepeatable="true"
            android:keyWidth="49.2%p"
            android:keyLabel="space" />
        <Key
            android:codes="46,44"
            android:keyLabel=". ,"
            android:popupCharacters=".,?!#@"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key
            android:codes="10"
            android:keyWidth="14.7%p"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/ic_nextline" />
    </Row>
</Keyboard>
