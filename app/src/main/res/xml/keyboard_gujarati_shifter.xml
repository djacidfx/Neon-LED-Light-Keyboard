<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:horizontalGap="0%p"
    android:keyHeight="52dp"
    android:verticalGap="4dp">
    <Row
        android:horizontalGap="0.1%p"
        android:keyWidth="8.1%p">
        <Key
            android:horizontalGap="1%p"
            android:keyEdgeFlags="left"
            android:keyLabel="ઔ" />
        <Key android:keyLabel="ઐ" />
        <Key android:keyLabel="આ" />
        <Key android:keyLabel="ઈ" />
        <Key android:keyLabel="ઊ" />
        <Key android:keyLabel="ભ" />
        <Key android:keyLabel="ઙ" />
        <Key android:keyLabel="ઘ" />
        <Key android:keyLabel="ધ" />
        <Key android:keyLabel="ઝ" />
        <Key android:keyLabel="ઢ" />
        <Key
            android:keyEdgeFlags="right"
            android:keyLabel="ઞ" />
    </Row>
    <Row
        android:horizontalGap="0.1%p"
        android:keyWidth="8.8%p">
        <Key
            android:horizontalGap="1%p"
            android:keyEdgeFlags="left"
            android:keyLabel="ઓ" />
        <Key android:keyLabel="એ" />
        <Key android:keyLabel="અ" />
        <Key android:keyLabel="ઇ" />
        <Key android:keyLabel="ઉ" />
        <Key android:keyLabel="ફ" />
        <Key android:keyLabel="ખ" />
        <Key android:keyLabel="થ" />
        <Key android:keyLabel="છ" />
        <Key android:keyLabel="ઠ" />
        <Key
            android:keyEdgeFlags="right"
            android:keyLabel="ઑ" />
    </Row>
    <Row
        android:horizontalGap="0.1%p"
        android:keyWidth="9.7%p">
        <Key
            android:codes="-1"
            android:horizontalGap="1%p"
            android:isModifier="true"
            android:isSticky="true"
            android:keyWidth="13.5%p"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/ic_shiftup" />
        <Key
            android:horizontalGap="1%p"
            android:keyLabel="ઁ" />
        <Key android:keyLabel="ણ" />
        <Key android:keyLabel="ળ" />
        <Key android:keyLabel="શ" />
        <Key android:keyLabel="ષ" />
        <Key android:keyLabel="।" />
        <Key android:keyLabel="ઋ" />
        <Key
            android:codes="-5"
            android:horizontalGap="1%p"
            android:isRepeatable="true"
            android:keyWidth="14%p"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/ic_backspace" />
    </Row>
    <Row
        android:horizontalGap="0.1%p"
        android:keyWidth="9.7%p"
        android:rowEdgeFlags="bottom">
        <Key
            android:codes="-2"
            android:horizontalGap="1%p"
            android:keyWidth="14.7%p"
            android:keyLabel="123" />
        <Key
            android:codes="-101"
            android:keyIcon="@drawable/icon_emoj1" />
        <Key
            android:codes="32"
            android:isRepeatable="true"
            android:keyWidth="49.2%p"
            android:keyLabel="space" />
        <Key
            android:codes="46,44"
            android:keyLabel=". ,"
            android:popupCharacters=".,?!#@"
            android:popupKeyboard="@xml/keyboard_popup_template" />
        <Key
            android:codes="10"
            android:keyWidth="14.7%p"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/ic_nextline" />
    </Row>
</Keyboard>
